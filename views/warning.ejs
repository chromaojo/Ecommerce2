<% if (messages ) { %>

    <div class="alert alert-primary alert-dismissible fade show text-center p-4" role="alert">
      <%= messages %>
      <a href="<%= link %>">
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
          
        </button>
      </a>
    </div>
    <% } %>  